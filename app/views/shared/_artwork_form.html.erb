<%= form_with(model: @artwork, local: true) do |f| %>
	<%= render 'shared/error_messages', object: f.object %>
	<div class="content_title">
		<%= f.text_field :title, placeholder: "Artwork title..." %>
	</div>
	<div class="content_field">
		<%= f.text_area :content, placeholder: "Artwork description..." %>
	</div>
	<%= f.submit "Submit", class: "btn btn-primary submit_button" %> 
<% end %>