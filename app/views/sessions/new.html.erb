<% provide(:title, "Log in") %>
<h1>Admin Log in</h1>

<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<%= form_with(url: login_path, scope: :session, local: true) do |f| %>
			<%= f.label :email %>
			<%= f.email_field :email, class: 'form-control' %>
			<%= f.label :password %>
			<%= f.password_field :password, class: 'form-control password-entry' %>
			<%= f.submit "Log in", class: "btn btn-primary login_button" %>
		<% end %>
	</div>
</div>